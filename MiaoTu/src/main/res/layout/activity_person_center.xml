                <TextView
                    android:id="@+id/tv_start"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/iv_start"
                    android:text="TA发起的约游"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/icon_go" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/bg_line" />

            <RelativeLayout
                android:id="@+id/rl_join"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@android:color/white"
                android:visibility="visible">

                <ImageView
                    android:id="@+id/iv_join"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/mine_yibaomingde" />

                <TextView
                    android:id="@+id/tv_sign"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/iv_join"
                    android:text="TA报名的约游"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/icon_go" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/bg_line" />

            <RelativeLayout
                android:id="@+id/rl_like"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@android:color/white"
                android:visibility="visible">

                <ImageView
                    android:id="@+id/iv_like"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/mine_xihuande" />

                <TextView
                    android:id="@+id/tv_like"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/iv_like"
                    android:text="TA喜欢的约游"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/icon_go" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/bg_line" />

            <TextView
                android:id="@+id/tv_tip_trends"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="动态"
                android:textColor="#b4b4b4"
                android:textSize="12sp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/bg_line" />

            <RelativeLayout
                android:id="@+id/rl_state"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:background="@android:color/white"
                android:visibility="visible">

                <ImageView
                    android:id="@+id/iv_state"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/mine_dongtai" />

                <TextView
                    android:id="@+id/tv_trends"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/iv_state"
                    android:text="TA的动态"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="10dp"
                    android:scaleType="fitCenter"
                    android:src="@drawable/icon_go" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@drawable/bg_line" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="个人资料"
                android:textColor="#b4b4b4"
                android:textSize="12sp" />


            <RelativeLayout
                android:id="@+id/rl_home"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:background="@android:color/white"
                android:visibility="gone">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@drawable/bg_line" />

                <TextView
                    android:id="@+id/tv_home"
                    android:layout_width="77dp"
                    android:layout_height="34dp"
                    android:gravity="center"
                    android:text="家乡"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:id="@+id/view1"
                    android:layout_width="1dp"
                    android:layout_height="20dp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_home"
                    android:background="#dcdcdc" />

                <TextView
                    android:id="@+id/tv_content_home"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/view1"
                    android:gravity="center_vertical"
                    android:text="杭州"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/bg_line" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_job"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:background="@android:color/white"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_job"
                    android:layout_width="77dp"
                    android:layout_height="34dp"
                    android:gravity="center"
                    android:text="职业"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:id="@+id/view5"
                    android:layout_width="1dp"
                    android:layout_height="20dp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_job"
                    android:background="#dcdcdc" />

                <TextView
                    android:id="@+id/tv_content_job"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/view5"
                    android:gravity="center_vertical"
                    android:text="企业管理者"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/bg_line" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_school"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:background="@android:color/white"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_school"
                    android:layout_width="77dp"
                    android:layout_height="34dp"
                    android:gravity="center"
                    android:text="学校"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:id="@+id/view2"
                    android:layout_width="1dp"
                    android:layout_height="20dp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_school"
                    android:background="#dcdcdc" />

                <TextView
                    android:id="@+id/tv_content_school"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/view2"
                    android:gravity="center_vertical"
                    android:text="27岁"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/bg_line" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_lifearea"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:background="@android:color/white"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_lifearea"
                    android:layout_width="77dp"
                    android:layout_height="34dp"
                    android:gravity="center"
                    android:text="生活在"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:id="@+id/view3"
                    android:layout_width="1dp"
                    android:layout_height="20dp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_lifearea"
                    android:background="#dcdcdc" />

                <TextView
                    android:id="@+id/tv_content_lifearea"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/view3"
                    android:gravity="center_vertical"
                    android:text="北京"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/bg_line" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_workarea"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:background="@android:color/white"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_workarea"
                    android:layout_width="77dp"
                    android:layout_height="34dp"
                    android:gravity="center"
                    android:text="工作在"
                    android:textColor="#646464"
                    android:textSize="14sp" />

                <View
                    android:id="@+id/view4"
                    android:layout_width="1dp"
                    android:layout_height="20dp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/tv_workarea"
                    android:background="#dcdcdc" />

                <TextView
                    android:id="@+id/tv_content_workarea"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/view4"
                    android:gravity="center_vertical"
                    android:text="国家大学科技园"
